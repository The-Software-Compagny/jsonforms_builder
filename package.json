    "@quasar/vite-plugin": "^1.10.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/language-plugin-pug": "^3.0.5",
    "cpr": "^3.0.1",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "quasar": "^2.18.2",
    "rimraf": "^6.0.1",
